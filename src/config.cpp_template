#include "data_objects.h"
#include "bat_charger.h"


// specify battery parameters to overwrite default settings here (be sure you understand what you are doing!)
void battery_init_user(battery_t *bat)
{
    battery_t bat_user;

    bat_user = *bat;        // start with already initialized default

/*
    bat_user.num_cells = ?;
    bat_user.capacity  = ?;
    bat_user.cell_voltage_max = ?;
    bat_user.cell_voltage_recharge = ?;
    bat_user.current_cutoff_CV = ?;
    bat_user.cell_voltage_load_disconnect = ?;
    bat_user.cell_voltage_load_reconnect = ?;

*/
    // try to update settings (checks input values for plausibility)
    battery_update_settings(bat, &bat_user);
}
